{
  "name": "apollo-react-components",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "gitmoji-cli": "^7.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "^3.1.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,md,mdx,js,jsx}": [
      "prettier --write"
    ]
  },
  "gitmoji": {
    "emojiFormat": "emoji"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
