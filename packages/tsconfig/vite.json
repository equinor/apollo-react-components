{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "Display": "Vite",
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}
